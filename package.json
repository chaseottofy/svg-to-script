  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "gh-pages": "^5.0.0",
    "html-minifier": "^4.0.0",
    "htmlnano": "^2.0.4",
    "minify": "^10.3.0",
    "tsup": "^7.1.0",
